    "@types/dompurify": "^3.2.0",
    "@types/js-yaml": "^4.0.9",
    "@types/react-copy-to-clipboard": "^5.0.7",
    "@types/uuid": "^9.0.8",
    "autoprefixer": "^10.4.21",
